            <xml-property name="queryText"><![CDATA[SELECT
	(
		SELECT CASE WHEN I18N_PROFIL.LANGUE_UTILISATEUR IS NULL OR I18N_PROFIL.LANGUE_UTILISATEUR = '' THEN I18N.LANGUE_UTILISATEUR ELSE I18N_PROFIL.LANGUE_UTILISATEUR END
		FROM I18N 
		LEFT OUTER JOIN I18N_PROFIL ON I18N_PROFIL.I18N = I18N.I18N AND I18N_PROFIL.ACTIF = 1 AND I18N_PROFIL.ID_PROFIL = PROFIL_ID 
    	LEFT OUTER JOIN PERSO ON PERSO.ID_PROFIL = I18N_PROFIL.ID_PROFIL AND PERSO.ID_PERSO = PERSO_ID
		WHERE I18N.I18N = 'EDITE_LE'
	) EDITE_LE,
	(
		SELECT CASE WHEN I18N_PROFIL.LANGUE_UTILISATEUR IS NULL OR I18N_PROFIL.LANGUE_UTILISATEUR = '' THEN I18N.LANGUE_UTILISATEUR ELSE I18N_PROFIL.LANGUE_UTILISATEUR END
		FROM I18N 
		LEFT OUTER JOIN I18N_PROFIL ON I18N_PROFIL.I18N = I18N.I18N AND I18N_PROFIL.ACTIF = 1 AND I18N_PROFIL.ID_PROFIL = PROFIL_ID 
    	LEFT OUTER JOIN PERSO ON PERSO.ID_PROFIL = I18N_PROFIL.ID_PROFIL AND PERSO.ID_PERSO = PERSO_ID
		WHERE I18N.I18N = 'PAGE'
	) PAGE,	
	(
		SELECT CASE WHEN I18N_PROFIL.LANGUE_UTILISATEUR IS NULL OR I18N_PROFIL.LANGUE_UTILISATEUR = '' THEN I18N.LANGUE_UTILISATEUR ELSE I18N_PROFIL.LANGUE_UTILISATEUR END
