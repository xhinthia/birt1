            <xml-property name="queryText"><![CDATA[SELECT
	DOCUMENT.DOCUMENT,
	DOCUMENT.DESI1,
	DV.VERSION,
	DOCUMENT.LANGUE,
	DOCUMENT.DATE_APPLICATION,
	DOCUMENT.DATE_PEREMPTION,
	DV.FICHIER_NOM,
	DV.FICHIER_EXTENSION
FROM DOCUMENT
LEFT OUTER JOIN DOCUMENT_VERSION DV ON DV.ID_DOCUMENT = DOCUMENT.ID_DOCUMENT AND DV.ACTIF = 1
WHERE DOCUMENT.ID_DOCUMENT IN (?)]]></xml-property>
