            <xml-property name="queryText"><![CDATA[SELECT
	SOCIETE.DESI1 AS SOCIETE_DESI1,
	ADR.ADRE1 AS SOCIETE_ADRE1,
	ADR.ADRE2 AS SOCIETE_ADRE2,
	ADR.CODEP AS SOCIETE_CODEP,
	ADR.VILLE AS SOCIETE_VILLE,
	ADR.PAYS AS SOCIETE_PAYS,
	ADR.TEL AS SOCIETE_TEL,
	ADR.FAX AS SOCIETE_FAX
FROM ADR
INNER JOIN SOCIETE ON SOCIETE.ID_ADR = ADR.ID_ADR AND SOCIETE.ACTIF = 1
INNER JOIN PERSO ON PERSO.ID_SOCIETE = SOCIETE.ID_SOCIETE AND PERSO.ACTIF = 1
WHERE PERSO.ID_PERSO = ?
]]></xml-property>
