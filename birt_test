            <method name="beforeOpen"><![CDATA[// On copie la requête
var query = this.queryText;

// On recherche la langue souhaitée, l'id du profil et l'id du perso
var langue = params["lg"].value;
var perso_id = parseInt(params["id_user"].value);
var profil_id = parseInt(params["id_profil"].value);

if (!langue) 
	langue = "FR";
	
if (!perso_id) 
	perso_id = 1;

if (!profil_id) 
	profil_id = 1;

langue = "LANG_" + langue;
perso_id = "" + perso_id;
profil_id = "" + profil_id;

// On remplace tous les LANGUE_UTILISATEUR par la langue souhaitée
query = query.replace("LANGUE_UTILISATEUR", langue);

// On remplace tous les PERSO_ID par l'utilisateur
query = query.replace("PERSO_ID", perso_id);

// On remplace tous les PROFIL_ID par le profil de l'utilisateur
query = query.replace("PROFIL_ID", profil_id);

// On affecte la requête
this.queryText = query;
]]></method>
